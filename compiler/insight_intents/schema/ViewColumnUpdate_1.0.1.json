{
  "intentName": "ViewColumnUpdate",
  "intentVersion": "1.0.1",
  "llmDescription": "查看专栏更新",
  "keywords": ["ViewColumnUpdate"],
  "parameters": {
    "type": "object",
    "propertyNames": {
      "enum": [
        "entityId"
      ]
    },
    "properties": {
      "entityId": {
        "description": "数据唯一标识，专栏更新的更新ID，根据ID跳转资讯详情页或者专栏主页。",
        "type": "string",
        "maxLength": 64
      }
    }
  },
  "result": {
    "type": "object",
    "propertyNames": {
      "enum": [
        "code",
        "result"
      ]
    },
    "required": [
      "code",
      "result"
    ],
    "properties": {
      "code": {
        "description": "意图调用返回的结果码，0代表成功。",
        "type": "number"
      },
      "result": {
        "description": "意图调用返回的数据，如果无数据则返回空。",
        "type": "object"
      }
    }
  }
}
