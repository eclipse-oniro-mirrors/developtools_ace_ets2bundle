  "scripts": {
    "run_abc:helloworld": "./node_modules/libarkts/node_modules/@panda/sdk/linux_host_tools/bin/ark --load-runtimes=ets --boot-panda-files=./node_modules/libarkts/node_modules/@panda/sdk/ets/etsstdlib.abc dist/modules_static.abc a.ETSGLOBAL::main",
